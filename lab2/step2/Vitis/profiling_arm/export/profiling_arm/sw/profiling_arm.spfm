<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="profiling_arm"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
profiling_arm
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="profiling_arm">
        <sdx:configuration sdx:name="profiling_arm"
                           sdx:displayName="profiling_arm"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>profiling_arm</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="profiling_arm/boot/profiling_arm.bif"
                            sdx:readme="profiling_arm/boot/generic.readme"
                            sdx:qemuArguments="profiling_arm/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="profiling_arm/boot/profiling_arm.bif"
                            sdx:readme="profiling_arm/boot/generic.readme"
                            sdx:qemuArguments="profiling_arm/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="profiling_arm/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="profiling_arm/standalone_domain/bspinclude/include"
                        sdx:bspConfig="profiling_arm/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
